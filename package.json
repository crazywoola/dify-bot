{
  "name": "dify-bot",
  "version": "1.0.0",
  "description": "Dify Bot",
  "main": "index.js",
  "author": "crazywoola",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.11.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "start": "node ./dist/index.js",
    "husky": "husky install && npx husky add .husky/pre-commit 'npx lint-staged'"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@slack/bolt": "^3.13.3",
    "@types/chalk": "^2.2.0",
    "@types/yargs": "^17.0.24",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1"
  }
}
